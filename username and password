//import scanner class
import java.util.*;
public class Main
{
    //create Object 
    static Scanner console = new Scanner(System.in);
	public static void main(String[] args) {
	   //declare variables
	   String userName;
	   String passWord;
	   //create do Statement 
	   do
	   {//begin do
	   //prompt user 
System.out.println(" Enter a username ");
	   //initialize variables
userName = console.next();
	   //prompt user for the guidelines
System.out.println(" Enter a password. Can't be the same same as user and the length must be greater than 12 characters!! ");
	   passWord = console.next();
	   
	   
	   } while (userName.compareToIgnoreCase(passWord)==0 || passWord.length() < 12);//end do
	   
	   
		
	}
}



public class Main
{
	public static void main(String[] args) 
	{//begin main method 
	HelloMessage();
	int x = 1;
	while (x <= 10)
	{//begin while
	System.out.println(" The Spanish number for " + x + " is " + Span2Eng(x));
	x++;
	
	
	}//end while
	GoodbyeMessage();
	
	
	
	
	}//end main
	    //create my method here 
	public static String Span2Eng (int x) //method heading!!! 
	{//begin method
	switch (x)
	    {//begin switch
	case 1: 
	    return "Uno";
	    
	case 2:
	    return "Dos";
	case 3:
	    return "Tres";
	case 4:
	    return "Quatro";
	    
	case 5: 
	    return "Cinco";
	    
	case 6:
	    return "Seis";
	    
	case 7:
	    return "Siete";
	    
	case 8: 
	    return "Ocho";
	    
	case 9:
	    return "Nueve";
	    
	case 10: 
	    return "Diez";
	    
	default:
	    return "Invalid";
	    
	    }//end switch
	    
	}//end method	
	
	
	
	//begin hello method
	public static void HelloMessage()
	{//begin HelloMessage
	System.out.println(" Welcome to Khalil's Spanish to English Converter 1-10!! ");
	}//end hello 
	
	
	//begin bye 
	public static void GoodbyeMessage()
	{//begin 
	System.out.println(" Goodbye, hope you were serviced! ");
	
	}//end
	
	
	
	
	
	
}

